package com.test.vikash;

public class LargestSumPair {
	private static char[] largestSumPair(int[] arr) {

		int maxFirst, maxSec;
		if(arr[0]>arr[1]) {
			maxFirst = arr[0]; 
			maxSec = arr[1];
			
		}else {
			maxFirst = arr[1]; 
			maxSec = arr[0]; 
			
		}
		return null;
	}
	public static void main(String[] args) {
		int arr[]= {1,4,6,7,2,9,3};
		System.out.println(largestSumPair(arr));
	}

	

}
